local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Kavo UI", "DarkTheme")

local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main Features")

MainSection:NewToggle("Super jump", "Enables super jump", function(state)
    if state then
        Library:Notify("Super jump is now enabled!")
    else
        Library:Notify("Super jump is now disabled!")
    end
end)
